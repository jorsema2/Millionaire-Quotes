{
  "manifest_version": 3,
  "name": "Rich People Quotes",
  "description": "Displays a random quote from famous rich people every time you open a new tab.",
  "version": "1.0",
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "permissions": [],
  "action": {
    "default_title": "Rich Quotes"
  },
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  }
}
