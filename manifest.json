{
  "manifest_version": 3,
  "name": "Millionaire Quotes",
  "description": "Boost your entrepreneurial mindset with motivational quotes from top millionaires on every new tab.",
  "version": "1.3.0",
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "permissions": [],
  "content_security_policy": {
    "extension_pages": "default-src 'self'; style-src 'self' https://fonts.googleapis.com; font-src 'self' https://fonts.gstatic.com"
  },
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "offline_enabled": true
}
